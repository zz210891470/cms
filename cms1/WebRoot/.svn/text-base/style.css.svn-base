/* CSS Document */
body{ margin:0px; padding:0px; font-size:12px;font-family:"宋体";color:#000000; background:url(images/body_bg.jpg) repeat-x top #e8e8e8;}
div,p,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,input,textarea,center {margin:0; padding:0;}
table,td,tr,th{font-size:12px; padding:0; margin:0;}
input, select {font-size:12px;line-height:16px; vertical-align:middle;}
table {empty-cells: show; border-collapse: collapse; }
img{ border:none;vertical-align:middle; }
a:link,a:visited,a:active{ color:#333333; text-decoration:none; }
a:hover{ color:#005eac; text-decoration:underline;}
ul,ol, li {list-style-type:none;}

