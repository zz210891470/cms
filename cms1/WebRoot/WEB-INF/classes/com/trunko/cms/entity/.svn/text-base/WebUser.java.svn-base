package com.trunko.cms.entity;

import java.sql.Timestamp;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

/**
 * 用户帐号对象
 * @author Administrator
 *
 */
public class WebUser implements java.io.Serializable {

	// Fields

	private Long id;
	private String yhm;   //用户名
	private String xm;    //姓名
	private String mm;    //密码
	private String tswt;  //提示问题
	private String tsda;  //提示答案
	private String yx;    //邮箱
	private String dh;    //电话	
	private String qx;    //权限
	private Timestamp dlsj;   //登录时间
	private Timestamp scdlsj; //上次登录时间
	private Integer dlcs;     //登录次数
	private Integer zt;     //状态
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getYhm() {
		return yhm;
	}
	public void setYhm(String yhm) {
		this.yhm = yhm;
	}
	public String getXm() {
		return xm;
	}
	public void setXm(String xm) {
		this.xm = xm;
	}
	public String getMm() {
		return mm;
	}
	public void setMm(String mm) {
		this.mm = mm;
	}
	public String getTswt() {
		return tswt;
	}
	public void setTswt(String tswt) {
		this.tswt = tswt;
	}
	public String getTsda() {
		return tsda;
	}
	public void setTsda(String tsda) {
		this.tsda = tsda;
	}
	public String getYx() {
		return yx;
	}
	public void setYx(String yx) {
		this.yx = yx;
	}
	public String getDh() {
		return dh;
	}
	public void setDh(String dh) {
		this.dh = dh;
	}
	public String getQx() {
		return qx;
	}
	public void setQx(String qx) {
		this.qx = qx;
	}
	public Timestamp getDlsj() {
		return dlsj;
	}
	public void setDlsj(Timestamp dlsj) {
		this.dlsj = dlsj;
	}
	public Timestamp getScdlsj() {
		return scdlsj;
	}
	public void setScdlsj(Timestamp scdlsj) {
		this.scdlsj = scdlsj;
	}
	public Integer getDlcs() {
		return dlcs;
	}
	public void setDlcs(Integer dlcs) {
		this.dlcs = dlcs;
	}
	public Integer getZt() {
		return zt;
	}
	public void setZt(Integer zt) {
		this.zt = zt;
	}


}